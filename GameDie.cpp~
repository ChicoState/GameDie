#include <cstdlib>
#include "GameDie.h"
#include <iostream>
using namespace std;

//class constructor that seeds the random number generator
GameDie::GameDie()
{

}

//generate a random number between 1-6 (inclusive) and display it
int GameDie::roll()
{
  	srand(time(NULL));
  	int r=(rand()%6)+1;
  	cout<<"Generated Random number is :" << r <<endl;
}
